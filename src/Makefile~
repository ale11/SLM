main: main.o TurbModel_WVM.o
	$(CPP) -o $(BINDIR)/main main.o TurbModel_WVM.o

prm_vars: prm_vars.o
	$(CPP) -o $(BINDIR)/prm_vars prm_vars.o

main.o: main.cpp TurbModel_WVM.hpp
	$(CPP) -c main.cpp -I.

TurbModel_WVM.o: TurbModel_WVM.cpp TurbModel_WVM.hpp
	$(CPP) -c TurbModel_WVM.cpp -I.

prm_vars.o: prm_vars.cpp
	$(CPP) -c prm_vars.cpp